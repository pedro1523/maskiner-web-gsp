<validators>
	<field  name="numOrdenTrabajo">
		<field-validator type="requiredstring" short-circuit="true">
			<message key="pages.reportes.repTecOT.errors.numOrdenTrabajo.required" />
		</field-validator>
		<field-validator type="regex">
			<param name="expression">OT[0-9]{4}</param>
			<message key="pages.reportes.repTecOT.errors.numOrdenTrabajo.regex" />
		</field-validator>
	</field>
</validators>

